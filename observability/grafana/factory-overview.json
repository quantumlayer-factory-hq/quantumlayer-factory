{
  "dashboard": {
    "id": null,
    "title": "QuantumLayer Factory - System Overview",
    "tags": ["quantumlayer", "factory", "overview"],
    "style": "dark",
    "timezone": "browser",
    "editable": true,
    "graphTooltip": 0,
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "refresh": "5s",
    "panels": [
      {
        "id": 1,
        "title": "Generation Rate",
        "type": "stat",
        "targets": [
          {
            "expr": "rate(qlf_generations_total[5m])",
            "legendFormat": "Generations/sec"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "reqps",
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": 5},
                {"color": "red", "value": 10}
              ]
            }
          }
        },
        "gridPos": {"h": 8, "w": 6, "x": 0, "y": 0}
      },
      {
        "id": 2,
        "title": "Success Rate",
        "type": "stat",
        "targets": [
          {
            "expr": "rate(qlf_generations_total{result=\"success\"}[5m]) / rate(qlf_generations_total[5m]) * 100",
            "legendFormat": "Success %"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {"color": "red", "value": null},
                {"color": "yellow", "value": 90},
                {"color": "green", "value": 95}
              ]
            }
          }
        },
        "gridPos": {"h": 8, "w": 6, "x": 6, "y": 0}
      },
      {
        "id": 3,
        "title": "Active Workflows",
        "type": "stat",
        "targets": [
          {
            "expr": "qlf_active_workflows",
            "legendFormat": "Active"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "color": {
              "mode": "palette-classic"
            }
          }
        },
        "gridPos": {"h": 8, "w": 6, "x": 12, "y": 0}
      },
      {
        "id": 4,
        "title": "Preview Environments",
        "type": "stat",
        "targets": [
          {
            "expr": "qlf_preview_environments_active",
            "legendFormat": "Active Previews"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "color": {
              "mode": "palette-classic"
            }
          }
        },
        "gridPos": {"h": 8, "w": 6, "x": 18, "y": 0}
      },
      {
        "id": 5,
        "title": "Generation Duration",
        "type": "timeseries",
        "targets": [
          {
            "expr": "histogram_quantile(0.50, rate(qlf_generation_duration_seconds_bucket[5m]))",
            "legendFormat": "p50"
          },
          {
            "expr": "histogram_quantile(0.95, rate(qlf_generation_duration_seconds_bucket[5m]))",
            "legendFormat": "p95"
          },
          {
            "expr": "histogram_quantile(0.99, rate(qlf_generation_duration_seconds_bucket[5m]))",
            "legendFormat": "p99"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "s",
            "color": {
              "mode": "palette-classic"
            }
          }
        },
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 8}
      },
      {
        "id": 6,
        "title": "Error Rate by Component",
        "type": "timeseries",
        "targets": [
          {
            "expr": "rate(qlf_errors_total[5m]) by (component)",
            "legendFormat": "{{component}}"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "reqps",
            "color": {
              "mode": "palette-classic"
            }
          }
        },
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 8}
      },
      {
        "id": 7,
        "title": "Generations by Language",
        "type": "piechart",
        "targets": [
          {
            "expr": "sum by (language) (rate(qlf_generations_total[1h]))",
            "legendFormat": "{{language}}"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "color": {
              "mode": "palette-classic"
            }
          }
        },
        "gridPos": {"h": 8, "w": 8, "x": 0, "y": 16}
      },
      {
        "id": 8,
        "title": "Generations by Framework",
        "type": "piechart",
        "targets": [
          {
            "expr": "sum by (framework) (rate(qlf_generations_total[1h]))",
            "legendFormat": "{{framework}}"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "color": {
              "mode": "palette-classic"
            }
          }
        },
        "gridPos": {"h": 8, "w": 8, "x": 8, "y": 16}
      },
      {
        "id": 9,
        "title": "System Resource Usage",
        "type": "timeseries",
        "targets": [
          {
            "expr": "rate(process_cpu_seconds_total[5m]) * 100",
            "legendFormat": "CPU %"
          },
          {
            "expr": "process_resident_memory_bytes / 1024 / 1024",
            "legendFormat": "Memory MB"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            }
          }
        },
        "gridPos": {"h": 8, "w": 8, "x": 16, "y": 16}
      },
      {
        "id": 10,
        "title": "Package Creation Rate",
        "type": "timeseries",
        "targets": [
          {
            "expr": "rate(qlf_packages_created_total[5m])",
            "legendFormat": "Packages/sec"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "reqps",
            "color": {
              "mode": "palette-classic"
            }
          }
        },
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 24}
      },
      {
        "id": 11,
        "title": "Deployment Success Rate",
        "type": "timeseries",
        "targets": [
          {
            "expr": "rate(qlf_deployments_total{result=\"success\"}[5m]) / rate(qlf_deployments_total[5m]) * 100",
            "legendFormat": "Success %"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {"color": "red", "value": null},
                {"color": "yellow", "value": 90},
                {"color": "green", "value": 95}
              ]
            }
          }
        },
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 24}
      }
    ],
    "templating": {
      "list": [
        {
          "name": "service",
          "type": "query",
          "query": "label_values(qlf_generations_total, service)",
          "refresh": 1,
          "includeAll": true,
          "multi": true
        },
        {
          "name": "language",
          "type": "query",
          "query": "label_values(qlf_generations_total, language)",
          "refresh": 1,
          "includeAll": true,
          "multi": true
        }
      ]
    },
    "annotations": {
      "list": [
        {
          "name": "Deployments",
          "datasource": "Prometheus",
          "expr": "changes(qlf_deployments_total[1m])",
          "titleFormat": "Deployment",
          "textFormat": "{{namespace}}: {{app_name}}"
        }
      ]
    }
  }
}