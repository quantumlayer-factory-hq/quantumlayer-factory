{
  "dashboard": {
    "id": null,
    "title": "QuantumLayer Factory - Agent Performance",
    "tags": ["quantumlayer", "agents", "performance"],
    "style": "dark",
    "timezone": "browser",
    "editable": true,
    "graphTooltip": 0,
    "time": {
      "from": "now-2h",
      "to": "now"
    },
    "refresh": "10s",
    "panels": [
      {
        "id": 1,
        "title": "Agent Execution Rate",
        "type": "timeseries",
        "targets": [
          {
            "expr": "rate(qlf_agent_execution_duration_seconds_count[5m]) by (agent_type)",
            "legendFormat": "{{agent_type}}"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "reqps",
            "color": {
              "mode": "palette-classic"
            }
          }
        },
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 0}
      },
      {
        "id": 2,
        "title": "Agent Success Rate",
        "type": "timeseries",
        "targets": [
          {
            "expr": "rate(qlf_agent_execution_duration_seconds_count{result=\"success\"}[5m]) / rate(qlf_agent_execution_duration_seconds_count[5m]) * 100 by (agent_type)",
            "legendFormat": "{{agent_type}} success %"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "color": {
              "mode": "palette-classic"
            },
            "thresholds": {
              "steps": [
                {"color": "red", "value": null},
                {"color": "yellow", "value": 90},
                {"color": "green", "value": 95}
              ]
            }
          }
        },
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 0}
      },
      {
        "id": 3,
        "title": "Agent Execution Duration",
        "type": "timeseries",
        "targets": [
          {
            "expr": "histogram_quantile(0.50, rate(qlf_agent_execution_duration_seconds_bucket[5m])) by (agent_type)",
            "legendFormat": "{{agent_type}} p50"
          },
          {
            "expr": "histogram_quantile(0.95, rate(qlf_agent_execution_duration_seconds_bucket[5m])) by (agent_type)",
            "legendFormat": "{{agent_type}} p95"
          },
          {
            "expr": "histogram_quantile(0.99, rate(qlf_agent_execution_duration_seconds_bucket[5m])) by (agent_type)",
            "legendFormat": "{{agent_type}} p99"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "s",
            "color": {
              "mode": "palette-classic"
            }
          }
        },
        "gridPos": {"h": 8, "w": 24, "x": 0, "y": 8}
      },
      {
        "id": 4,
        "title": "Code Generation Output",
        "type": "timeseries",
        "targets": [
          {
            "expr": "avg by (agent_type) (qlf_agent_execution_duration_seconds{lines_of_code!=\"\"})",
            "legendFormat": "{{agent_type}} avg lines"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "color": {
              "mode": "palette-classic"
            }
          }
        },
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 16}
      },
      {
        "id": 5,
        "title": "Files Generated by Agent",
        "type": "timeseries",
        "targets": [
          {
            "expr": "avg by (agent_type) (qlf_agent_execution_duration_seconds{files_count!=\"\"})",
            "legendFormat": "{{agent_type}} avg files"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "color": {
              "mode": "palette-classic"
            }
          }
        },
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 16}
      },
      {
        "id": 6,
        "title": "Agent Usage by Language",
        "type": "heatmap",
        "targets": [
          {
            "expr": "sum by (agent_type, language) (rate(qlf_agent_execution_duration_seconds_count[1h]))",
            "legendFormat": "{{agent_type}}/{{language}}"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "reqps",
            "color": {
              "mode": "spectrum"
            }
          }
        },
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 24}
      },
      {
        "id": 7,
        "title": "Agent Usage by Framework",
        "type": "heatmap",
        "targets": [
          {
            "expr": "sum by (agent_type, framework) (rate(qlf_agent_execution_duration_seconds_count[1h]))",
            "legendFormat": "{{agent_type}}/{{framework}}"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "reqps",
            "color": {
              "mode": "spectrum"
            }
          }
        },
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 24}
      },
      {
        "id": 8,
        "title": "LLM vs Template Usage",
        "type": "piechart",
        "targets": [
          {
            "expr": "sum by (llm_used) (rate(qlf_agent_execution_duration_seconds_count[1h]))",
            "legendFormat": "LLM: {{llm_used}}"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "color": {
              "mode": "palette-classic"
            }
          }
        },
        "gridPos": {"h": 8, "w": 8, "x": 0, "y": 32}
      },
      {
        "id": 9,
        "title": "Agent Error Types",
        "type": "table",
        "targets": [
          {
            "expr": "sum by (agent_type, error_type) (rate(qlf_agent_execution_duration_seconds_count{result=\"error\"}[1h]))",
            "legendFormat": "{{agent_type}}: {{error_type}}",
            "format": "table"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "reqps",
            "color": {
              "mode": "palette-classic"
            }
          }
        },
        "gridPos": {"h": 8, "w": 16, "x": 8, "y": 32}
      },
      {
        "id": 10,
        "title": "Top Performing Agents",
        "type": "bargauge",
        "targets": [
          {
            "expr": "topk(5, sum by (agent_type) (rate(qlf_agent_execution_duration_seconds_count{result=\"success\"}[1h])))",
            "legendFormat": "{{agent_type}}"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "reqps",
            "color": {
              "mode": "continuous-GrYlRd"
            }
          }
        },
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 40}
      },
      {
        "id": 11,
        "title": "Agent Resource Efficiency",
        "type": "timeseries",
        "targets": [
          {
            "expr": "rate(qlf_agent_execution_duration_seconds_sum[5m]) / rate(qlf_agent_execution_duration_seconds_count[5m]) by (agent_type)",
            "legendFormat": "{{agent_type}} avg duration"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "s",
            "color": {
              "mode": "palette-classic"
            }
          }
        },
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 40}
      }
    ],
    "templating": {
      "list": [
        {
          "name": "agent_type",
          "type": "query",
          "query": "label_values(qlf_agent_execution_duration_seconds, agent_type)",
          "refresh": 1,
          "includeAll": true,
          "multi": true
        },
        {
          "name": "language",
          "type": "query",
          "query": "label_values(qlf_agent_execution_duration_seconds, language)",
          "refresh": 1,
          "includeAll": true,
          "multi": true
        },
        {
          "name": "framework",
          "type": "query",
          "query": "label_values(qlf_agent_execution_duration_seconds, framework)",
          "refresh": 1,
          "includeAll": true,
          "multi": true
        }
      ]
    },
    "annotations": {
      "list": [
        {
          "name": "Agent Errors",
          "datasource": "Prometheus",
          "expr": "changes(qlf_errors_total{component=\"agent_factory\"}[1m])",
          "titleFormat": "Agent Error",
          "textFormat": "{{agent_type}}: {{error_type}}"
        }
      ]
    }
  }
}